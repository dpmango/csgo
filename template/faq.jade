extends layout/main.jade
block body
	body
		include _includes/svg.jade
		include _includes/blocks/sidebar.jade
		include _includes/blocks/dropside.jade
		include _includes/blocks/nav.jade
		include _includes/blocks/footer.jade
		include _includes/blocks/faq.jade

		.page-wrapp
			+sidebar()
			+dropside()
			.page-content
				.content-without-footer
					.container
						+nav(3)
						+faq()
				.footer-wrap
					.container
						+footer()



		//- +modals()

		// scripts
		script(data-main="js/main" src='js/require.js')