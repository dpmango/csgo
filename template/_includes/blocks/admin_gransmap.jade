            form.gransmap__aside__create-map
                input.create-map__name(type="text", name="map-name", placeholder="Название")
                .create-map__img.file
                    input#map-photo(type="file" name="map-photo")
                    button.create-map__file.file__btn.btn.btn-lblue.b-btn(type="button")
                        | загрузить изображение
                        span.btn-gray__topleft
                        span.btn-gray__bottomright
                    .file__value
                button.create-map__btn.btn.btn-blue.b-btn(type="submit")
                    | Создаать карту
                    span.btn-gray__topleft
                    span.btn-gray__bottomright

            .gransmap__aside__create
                button.gransmap__aside__create__add.js-add-grans.btn.btn-lblue.b-btn(type="button")
                    | Добавить
                    span.btn-gray__topleft
                    span.btn-gray__bottomright

                form#create-grans
                    p.gransmap__aside__create__title Создать гранату
                    .gransmap__aside__create__coords.clearfix
                        label
                            span X1
                            input#user-x(type="text", value="")
                        label
                            span Y1
                            input#user-y(type="text", value="")
                    .gransmap__aside__create__coords.clearfix
                        label
                            span X2
                            input#destenation-x(type="text", value="")
                        label
                            span Y2
                            input#destenation-y(type="text", value="")
                    .gransmap__aside__create__select-wrap
                        select#grans-type
                            option(value="0") Вид гранаты
                            option(value="smoke", data-icon="cloud-icon") Дымовая
                            option(value="flash", data-icon="explotion-icon") Ослепительная
                            option(value="fire", data-icon="fire-icon") Боевая

                    input#youtube-link.gransmap__aside__create__youtube(type="text", placeholder="Youtube link")
                    button.btn.btn-lblue.b-btn(type="submit")
                        | создать
                        span.btn-gray__topleft
                        span.btn-gray__bottomright



        mixin gransmap__map__granitem(top1, left1, top2, left2, type, active, video, videoimg)
            if (active == true)
                .gransmap__map__granitem.active(data-gran=type, data-video=video, data-videoimg=videoimg)
                    a(href="#" style="top: #{top1}; left: #{left1};").gransmap__map__granitem__gran
                        if (type == 'smoke')
                            +icon('cloud-icon')
                        if (type == 'fire')
                            +icon('fire-icon')
                        if (type == 'flash')
                            +icon('explotion-icon')
                    .gransmap__map__granitem__player(style="top: #{top2}; left: #{left2};")
                        +icon('player-icon')
                        .gransmap__map__granitem__arrow
                            span
            else
                .gransmap__map__granitem(data-gran=type, data-video=video, data-videoimg=videoimg)
