                .promo__cases.flex-cases
                    mixin simple-case(data)
                        .promo__cases__col.__simple.flex-case-col
                            .promo-case.cases-sales__case
                                .promo-case__img
                                    .promo-case__img__case
                                        img(src="img/content/simple-case.png", alt="")
                                .promo-case__name.cases-sales__case-name
                                    span AK-47 Redline
                                .promo-case__count
                                    span 20 раз
                                .promo-case__data
                                    div
                                        span открыт 20 раз
                                        span прибыль 5 700 рублей
                                span.btn-gray__topleft
                                span.btn-gray__bottomright
                    +simple-case()
                    +simple-case()
                    +simple-case()

